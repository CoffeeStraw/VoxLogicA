load graph = "testGraph.json"

// near(p): chiusura topologica della regione p
// through(p,q): esiste un path passante solo per punti p che arriva a punti q

let p = ap("p")
let q = ap("q")

save "p" p
save "q" q

save "near p" near(p)
save "near q" near(q)

save "q reach p" through(q,p)
save "p reach q" through(p,q)

save "path from p to q" and(p,through(not(ff),q))

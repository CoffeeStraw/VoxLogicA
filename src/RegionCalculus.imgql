let pi(phi1,phi2) =  p(phi2,phi1)
let ppi(phi1,phi2) = pp(phi2,phi1)
let o(phi1,phi2) = nempty(phi1 & phi2)
let po(phi1,phi2) = o(phi1,phi2) .&. (!. p(phi1,phi2)) .&. (!.p (phi2,phi1))
let dr(phi1,phi2) = !. (o(phi1,phi2))
let eq(phi1,phi2) = equalB(phi1,phi2)
let dc(phi1,phi2) = nempty(phi1) .&. nempty(phi2) .&. empty(near(phi1) & phi2)
